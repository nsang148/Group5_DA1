	<queryString>
		<![CDATA[SELECT dbo.HOADON.MaHD, dbo.HOADON.NGAYTAO, dbo.HOADON.TONGTIEN, dbo.HOADON.NGAYTHANHTOAN, dbo.HOADONCHITIET.SOLUONG, dbo.HOADONCHITIET.DONGIA, dbo.HOPTHIT.Ten, dbo.HOPTHIT.Ma AS MASACH, dbo.HOPTHIT.GiaBan,dbo.HOADONCHITIET.SOLUONG*dbo.HOPTHIT.GiaBan AS THANHTIEN
FROM     dbo.HOADON INNER JOIN
                  dbo.HOADONCHITIET ON dbo.HOADON.Id = dbo.HOADONCHITIET.IdHd INNER JOIN
                  dbo.HOPTHIT ON dbo.HOADONCHITIET.IdHT = dbo.HOPTHIT.Id
where dbo.HOADON.MaHD=$P{sMAHD}]]>
